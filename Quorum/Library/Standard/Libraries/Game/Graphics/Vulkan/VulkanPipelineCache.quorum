package Libraries.Game.Graphics.Vulkan

class VulkanPipelineCache

    VulkanDevice device = undefined
    boolean initialized = false

    action Create(VulkanDevice device) returns boolean
        boolean success = CreateNative(device)

        if success
            me:device = device
            initialized = true
        end

        return success
    end

    private system action CreateNative(VulkanDevice device) returns boolean

end