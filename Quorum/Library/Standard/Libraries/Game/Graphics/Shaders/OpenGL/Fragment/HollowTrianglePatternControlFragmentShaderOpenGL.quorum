package Libraries.Game.Graphics.Shaders.OpenGL.Fragment

class HollowTrianglePatternControlFragmentShaderOpenGL is ControlFragmentShaderOpenGL, HollowTrianglePatternFragmentShaderPartOpenGL

    constant text SHADER_NAME = "Hollow Triangle Pattern Control Fragment Shader"

    on create
        SetName(SHADER_NAME)
    end

    private action GetExtraFunctions returns text
        return parent:HollowTrianglePatternFragmentShaderPartOpenGL:GetExtraFunctions()
    end

    private action GetExtraInputs returns text
        return parent:HollowTrianglePatternFragmentShaderPartOpenGL:GetExtraInputs()
    end

    private action GetAppendedCode returns text
        return parent:HollowTrianglePatternFragmentShaderPartOpenGL:GetAppendedCode()
    end

end