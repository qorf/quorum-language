package Libraries.Game.Graphics.Shaders.OpenGL.Fragment

class SolidCirclePatternControlFragmentShaderOpenGL is ControlFragmentShaderOpenGL, SolidCirclePatternFragmentShaderPartOpenGL

    constant text SHADER_NAME = "Solid Circle Pattern Control Fragment Shader"

    on create
        SetName(SHADER_NAME)
    end

    private action GetExtraFunctions returns text
        return parent:SolidCirclePatternFragmentShaderPartOpenGL:GetExtraFunctions()
    end

    private action GetExtraInputs returns text
        return parent:SolidCirclePatternFragmentShaderPartOpenGL:GetExtraInputs()
    end

    private action GetAppendedCode returns text
        return parent:SolidCirclePatternFragmentShaderPartOpenGL:GetAppendedCode()
    end

end