    action SetShape(CollisionShape2D shape)
        me:shape = shape:Copy()

        ManageLayers()
    end

    private action ManageLayers
        if layerSet
            if collidable
                if not nodesSet
                    CreateNodes(layer:GetCollisionManager():GetBroadphase(), transform)
                end
            else
                if nodesSet
                    DestroyNodes(layer:GetCollisionManager():GetBroadphase())
                end
            end
        end
    end
