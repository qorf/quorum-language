    private action GetAssignmentOpcode(integer opcode, integer secondOpcode, Type type) returns AssignmentOpcode
        AssignmentOpcode op
        op:SetJavaOpcode(opcode)
        op:SetJavaOpcode2(secondOpcode)
        op:SetType(type)
        return op
    end

