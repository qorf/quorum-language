use Libraries.Compute.Matrix

class Main 
    action Main
        Matrix matrix
        matrix:SetSize(3,3)
        matrix:Fill(1.1)
        CheckResult(matrix:ToText())
    end

    action CheckResult(text result)
        text answer = ""
        text cr = answer:GetCarriageReturn()
        answer = "|1.1, 1.1, 1.1|"+cr+"|1.1, 1.1, 1.1|"+cr+"|1.1, 1.1, 1.1|"
        if answer = result
            output "pass"
        else
            output "fail"
        end
    end
end