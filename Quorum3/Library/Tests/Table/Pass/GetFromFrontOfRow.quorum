use Libraries.Containers.Table
use Libraries.Containers.Iterator
use Libraries.Containers.Array
class Main
    action Main
        integer r1 = 0
        integer r2 = 0
        integer r3 = 0
        integer r4 = 0
        Table<integer> t
        t:SetSize(1,4)
        t:Set(0,0,1)
        t:Set(0,1,2)
        t:Set(0,2,3)
        t:Set(0,3,4)

        r1 = t:GetFromFrontOfRow(0)
        r2 = t:GetFromFrontOfRow(0)
        r3 = t:GetFromFrontOfRow(0)
        r4 = t:GetFromFrontOfRow(0)
        output r1
        output r2
        output r3
        output r4
    end
end