use Libraries.Containers.List

/**
 * Uses:
 * action Add( Type value )
 * action Get( integer index ) returns Type
**/

/**
 * Description:
 * The purpose of this test case is to test the Add action of the LinkedList
 * class.
**/

/**
 * Expected Results:
 * textTotal = "FirstPurpleDinosaurChairWhiteCouchLego"
**/

/*
 * @author Elliot Motl
*/
class Main
    action Main
       List<text> textList

       textList:Add("First")
       textList:Add("Purple")
       textList:Add("Dinosaur")
       textList:Add("Chair")
       textList:Add("White")
       textList:Add("Couch")
       textList:Add("Lego")


       text textTotal = ""
       integer i = 0
       repeat from i to 6
           textTotal = textTotal + textList:Get(i)
           i = i + 1
       end

       print textTotal
    end
end