use Libraries.Containers.List

/**
 * Uses:
 * action Clear()
 * action Get( integer index ) returns Type
 * action Add( Type value )
**/

/**
 * Description:
 * The purpose of this test case is to test the Clear action.  the list will
 * be cleared and then new values will be entered into the list.
**/

/**
 * Expected Results:
 * textTotal = "ThisIsATextLinkedList!"
**/

/*
 * @author Elliot Motl
*/
class Main
    action Main
       List<text> textList

       textList:Add("one")
       textList:Add("five")
       textList:Add("nine")
       textList:Add("one")
       textList:Add("eight")
       textList:Add("three")
       textList:Add("one")

       textList:Empty()

       textList:Add("This")
       textList:Add("Is")
       textList:Add("A")
       textList:Add("Text")
       textList:Add("Linked")
       textList:Add("List")
       textList:Add("!")

       text textTotal = ""
       integer i = 0
       repeat from i to 6
           textTotal = textTotal + textList:Get(i)
           i = i + 1
       end
    end
end