/*
Expected output:
detect
always
*/

use Libraries.Language.Errors.all

class Main
    action Main
        check
            integer k = cast(integer, "3.7")
        detect e of type CastError
            output "detect"
        always
            integer z = 906
            if z = 907
                output "shouldn't be here"
            elseif z = 906
                output "always"
            end
        end
    end
end