use Libraries.Containers.Stack

/**
 * Uses:
 * action Remove( Type value ) returns Type
 * action RemoveFromTop() returns Type
**/

/**
 * Description:
 * The purpose of this test case is to test the Remove action.
**/

/**
 * Expected Results:
 * result = "FiveFourTwoOne"
**/

/**
 * @author Elliot Motl
**/
class Main
    action Main
       Stack<text> s

       s:Add("One")
       s:Add("Two")
       s:Add("Three")
       s:Add("Four")
       s:Add("Five")

       s:Remove("Three")

       text result = ""
       integer i = 0
       repeat 4 times 
           result = result + s:Pop()
           i = i + 1
       end

       print result
    end
end