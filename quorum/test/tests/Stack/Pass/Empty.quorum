use Libraries.Containers.Stack

/**
 * Uses:
 * action AddToTop( Type value )
 * action Remove() returns Type
 * action Clear()
**/

/**
 * Description:
 * this test case tests the Clear action.
**/

/**
 * Expected Results:
 * result = "FiveFourThreeTwoOne"
**/

/*
 * @author Elliot Motl
*/
class Main
    action Main
       Stack<text> s1

       s1:Push("1")
       s1:Push("2")
       s1:Push("3")
       s1:Push("4")
       s1:Push("5")

       s1:Empty()

       s1:Push("One")
       s1:Push("Two")
       s1:Push("Three")
       s1:Push("Four")
       s1:Push("Five")

       text result = ""
       integer i = 0
       repeat 5 times
            result = result + s1:Pop()
            i = i + 1
       end

       print result
    end
end