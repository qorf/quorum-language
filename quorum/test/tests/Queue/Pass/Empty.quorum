use Libraries.Containers.Queue

/**
 * Uses:
 * action AddToTop( Type value )
 * action Remove() returns Type
 * action Clear()
**/

/**
 * Description:
 * this test case tests the Clear action.
**/

/**
 * Expected Results:
 * result = "FiveFourThreeTwoOne"
**/

/*
 * @author Elliot Motl
*/
class Main
    action Main
       Queue<text> s1

       s1:Add("1")
       s1:Add("2")
       s1:Add("3")
       s1:Add("4")
       s1:Add("5")

       s1:Empty()

       s1:Add("One")
       s1:Add("Two")
       s1:Add("Three")
       s1:Add("Four")
       s1:Add("Five")

       text result = ""
       integer i = 0
       repeat 5 times
            result = result + s1:RemoveFromFront()
            i = i + 1
       end

       print result
    end
end